cabal-version:       >=1.10

executable TestCSV
  hs-source-dirs:      .
  main-is:             TestCSV.hs
  default-language:    Haskell2010
  build-depends:       base >= 4.7 && < 5, csv, either-unwrap

executable TestTextJSON
  hs-source-dirs:      .
  main-is:             TestTextJSON.hs
  default-language:    Haskell2010
  build-depends:       base >= 4.7 && < 5, json

executable TestAESON
  hs-source-dirs:      .
  main-is:             TestAESON.hs
  default-language:    Haskell2010
  build-depends:       base >= 4.7 && < 5, aeson

executable CleanText
  hs-source-dirs:      .
  main-is:             CleanText.hs
  default-language:    Haskell2010
  build-depends:       base >= 4.7 && < 5, split, text, MissingH
